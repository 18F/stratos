      "parameters": [
        {
          "name": "PORT"
        },
        {
          "name": "MYSQL_ROOT_PASSWORD"
        },
        {
          "name": "MYSQL_DATABASE"
        },
        {
          "name": "MYSQL_USER"
        },
        {
          "name": "MYSQL_PASSWORD"
        }
      ]
