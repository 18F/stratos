      "min_RAM_mb": 256,
      "min_disk_gb": 1,
      "min_VCPU": 1,
      "platform": "linux-x86_64",
      "workload_type": "container",
      "min_instances": 1,
      "max_instances": 1,
      "service_ports": [
        {
          "name": "api",
          "protocol": "TCP",
          "source_port": 3000,
          "target_port": 3000,
          "public": true
