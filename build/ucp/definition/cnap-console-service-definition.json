    {
      "name": "auth-service",
      "version": "1.0.0",
      "vendor": "HPE",
      "external_name": "auth-service",
      "image": "docker-registry.helion.space:443/helioncf/cnap-console-mock-auth:latest",
      "min_RAM_mb": 256,
      "min_disk_gb": 1,
      "min_VCPU": 1,
      "platform": "linux-x86_64",
      "workload_type": "container",
      "min_instances": 1,
      "max_instances": 1,
      "service_ports": [
        {
          "name": "mock-auth",
          "protocol": "TCP",
          "source_port": 3001,
          "target_port": 3001,
          "public": true
