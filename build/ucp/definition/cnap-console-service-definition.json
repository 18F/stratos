    {
      "name": "stolon-keeper",
      "version": "0.1.0",
      "vendor": "HPE",
      "external_name": "stolon-keeper",
      "image": "docker-registry.helion.space:443/stolon",
      "min_RAM_mb": 512,
      "min_disk_gb": 1,
      "platform": "linux-x86_64",
